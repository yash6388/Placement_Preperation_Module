problem- 1: search a 2D Matrix
sol-
bool searchMatrix(vector<vector<int>>& matrix, int target) {
         int row = matrix.size();
        int col = matrix[0].size();
        
        int start = 0;
        int end = row*col-1;
        
        int mid = start + (end-start)/2;
        while(start<=end){
            int element = matrix[mid/col][mid%col];
            if(element == target){
                return 1;
            }
            if(element<target){
                start = mid+1;
            }
            else
                end = mid-1;
            
            mid = start + (end-start)/2;
            
        }
        return 0;
    }
Problem 2-Majority Element
sol-
int majorityElement(vector<int>& nums) {
        int count = 0;
        int candidate = 0;
        for(auto num : nums){
            if(count == 0){
                candidate = num;
            }
            if(candidate == num){
                count++;
            }
            else{
                count--;
            }
        }
        return candidate;
    }
